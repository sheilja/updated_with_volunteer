
<!DOCTYPE html>
<html lang="en">

<%- include("../style.ejs") %>
<body>
  <div class="container-scroller">
    <!-- partial:partials/_navbar.html -->
  
    <!-- partial -->

    <%- include("../head1.ejs") %>
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar.html -->
     
      <!-- partial -->
     
      <%- include("../sidebar") %>
     
      <div class="main-panel">
        <div class="content-wrapper">


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title"> Request Details</h4>
        
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th>
                  Event Name
                </th>
                <th>
                  View Requests
                </th>
              
              </tr>
            </thead>
            <tbody>
                <% for(let i=0;i < info.length; i++) { %>
               <% info[i].Sub_Events.forEach(data=>{  %>

                 
              <tr>
                <td class="py-1">
                  <%=data.Event_name %>
                </td>  
                <td>
                    <a class="btn btn-info" href="/view_Requests?event_name=<%= data.Event_name %>">View Requests</a>
                </td>
                
              </tr>
              <% })}%>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <%- include("../footer") %>
  </div>
  </div>
</div>
</div>
</body>
</html>