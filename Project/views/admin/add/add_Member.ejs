
<!DOCTYPE html>
<html lang="en">

<%- include("../style.ejs") %>
<body>
  <div class="container-scroller">
    <!-- partial:partials/_navbar.html -->
  
    <!-- partial -->

    <%- include("../head1.ejs") %>
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar.html -->
     
      <!-- partial -->
     
      <%- include("../sidebar") %>
     
      <div class="main-panel">
        <div class="content-wrapper">


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Add Member</h4>
        
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th>
                  Name
                </th>
                <th>
                  Email
                </th>
                <th>
                  Contact
                </th>
                <th>
                  Add
                </th>
               
              
              </tr>
            </thead>
            <tbody>
                <% for(let i=0;i < info.length; i++) { %>
                   
                   
              <tr>
                <td class="py-1">
                  <%= info[i].name %>
                </td>
             
                <td>
                  <%= info[i].email %>
                </td>
                <td>
                  <%= info[i].contact %>
                </td>
                <td>
                   <a class="btn btn-info" href="/process_Member?stud=<%=info[i]._id%>">Accept</a>
                </td>
                
              </tr>
              <%   }%>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <%- include("../footer") %>
  </div>
  </div>
</div>
</div>
</body>
</html>